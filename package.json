{
  "name": "videoary-js",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "browserslist": ["last 2 versions"],
  "files": [
    "dist/*.css",
    "dist/*.js",
    "dist/*.ts"
  ],
  "main": "./dist/videoary.umd.cjs",
  "module": "./dist/videoary.js",
  "types": "./dist/videoary.d.ts",
  "exports": {
    ".": {
      "import": "./dist/videoary.js",
      "require": "./dist/videoary.umd.cjs"
    },
    "./dist/videoary.min.css": {
      "import": "./dist/videoary.min.css"
    }
  },
  "style": "./dist/videoary.min.css",
  "scripts": {
    "dev": "vite",
    "dev:compile": "tsc ./src/script.ts --target es6 --outdir ./src/dist --watch",
    "build": "vite build",
    "lint": "tsc",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@types/node": "^18.14.4",
    "autoprefixer": "^10.4.13",
    "sass": "^1.57.1",
    "typescript": "^4.9.4",
    "vite": "^3.1.0",
    "vite-plugin-dts": "^2.0.2"
  },
  "dependencies": {
    "esbuild": "0.16.14",
    "hls.js": "^1.3.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mohamadadithya/videoary"
  },
  "author": "Mohamad Adithya",
  "bugs": {
    "url": "https://github.com/mohamadadithya/videoary/issues"
  },
  "homepage": "https://github.com/mohamadadithya/videoary#readme",
  "license": "MIT"
}